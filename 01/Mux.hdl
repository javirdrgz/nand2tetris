/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Put your code here:
    Not(in=sel, out=notSel);
    Or(a=a, b=sel, out=selOrA);
    Or(a=notSel, b=b, out=notSelOrB);
    And(a=selOrA, b=notSelOrB, out=out);
}